<Window x:Class="WpfAddressBook.Views.ShellWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:prism="http://prismlibrary.com/"
        xmlns:const="clr-namespace:ClientInfrastructure.Constants;assembly=ClientInfrastructure"
        mc:Ignorable="d"
        Title="Address Book" Height="450" Width="800">
    <Grid>
        <ContentControl prism:RegionManager.RegionName="{x:Static const:RegionNames.MainWindowRegion}"
                        prism:RegionManager.RegionContext="{Binding RegionContext, Mode=TwoWay}" />
    </Grid>
</Window>
